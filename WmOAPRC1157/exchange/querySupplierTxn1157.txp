SUP.INVOICE_AMOUNT_LIMIT,
SUP.EXCHANGE_DATE_CODE,
SUP.HOLD_ALL_PAYMENTS,
SUP.HOLD_FUTURE_PAYMENTS,
SUP.HOLD_REASON,
SUP.DISTRIBUTION_SET_NAME,
SUP.SUPPLIER_LIABILITY_ACCOUNT,
SUP.NUMBER_1099,
SUP.TYPE_1099,
SUP.WITHHOLDING_STATUS,
SUP.WITHHOLDING_START_DATE,
SUP.IRS_ORGANIZATION_TYPE,
