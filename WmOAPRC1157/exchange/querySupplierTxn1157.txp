SUP.HOLD_DATE,
SUP.TERMS_DATE_BASIS,
SUP.PRICE_TOLERANCE,
SUP.INSPECTION_REQUIRED,
SUP.RECEIPT_REQUIRED_FLAG,
SUP.QUANTITY_RECEIVED_TOLERANCE,
SUP.DAYS_EARLY_RECEIPT_ALLOWED,
SUP.DAYS_LATE_RECEIPT_ALLOWED,
SUP.ALLOW_SUBSTITUTE_RECEIPTS,
SUP.ALLOW_UNORDERED_RECEIPTS,
SUP.HOLD_UNMATCHED_INVOICES,
SUP.EXCLUSIVE_PAYMENT,
SUP.TAX_VERIFICATION_DATE,
SUP.STATE_REPORTABLE,
SUP.FEDERAL_REPORTABLE,
SUP.OFFSET_VAT,
SUP.VAT_REGISTRATION_NUMBER,
SUP.AUTO_CALCULATE_INTEREST,
SUP.VALIDATION_NUMBER,
SUP.EXCLUDE_FREIGHT_FROM_DISCOUNT,
SUP.TAX_REPORTING_NAME,
SUP.CHECK_DIGITS,
SUP.BANK_NUM,
SUP.AUTO_TAX_CALCULATION,
SUP.AUTO_TAX_CALCULATION_OVERRIDE,
SUP.AMOUNT_INCLUDES_TAX,
SUP.BANK_CHARGE_BEARER,
SUP.BANK_BRANCH_TYPE,
SUP.MATCH_OPTION,
SUP.FUTURE_DATED_PAYMENTS_ACCOUNT,
SUP.CREATE_DEBIT_MEMO,
SUP.OFFSET_FLAG 
FROM WM_PO_SUPPLIER_QRY_VW SUP, WM_PO_SUPPLIER_SITES_ALL_VW SITE WHERE SUP.VENDOR_NUMBER LIKE NVL('&amp;SUPPLIER_NUMBER*',SUP.VENDOR_NUMBER)
AND NVL(SUP.NUMBER_1099,'XXXXX') LIKE NVL('&amp;TAXPAYER_ID*',NVL(SUP.NUMBER_1099,'XXXXX'))
AND NVL(SUP.END_DATE_ACTIVE,SYSDATE) between 
NVL(TO_DATE('&amp;INACTIVE_DATE_FROM*'),
NVL(SUP.END_DATE_ACTIVE,SYSDATE)) AND 
NVL(TO_DATE('&amp;INACTIVE_DATE_TO*'),
NVL(SUP.END_DATE_ACTIVE,SYSDATE)) 
AND NVL(SUP.VAT_REGISTRATION_NUMBER,'XXXXX') LIKE NVL('&amp;TAX_REGISTRATION_NUMBER*',NVL(SUP.VAT_REGISTRATION_NUMBER,'XXXXX'))
AND SITE.VENDOR_ID(+)=SUP.VENDOR_ID
AND NVL(SITE.VENDOR_SITE_CODE,'XXXX') LIKE NVL(UPPER('&amp;SUPPLIER_SITE_CODE*'),NVL(SITE.VENDOR_SITE_CODE,'XXXX'))</value>
